{
  "data": {
    "source": {
      "name": "google_2019",
      "directory_path": "../input/data/google_third_preprocess_cluster_selection_pca_standarized",
      "time_feature": "time_stamp",
      "target_feature": "cpu_maximum_usage",
      "batch_size": 512,
      "preprocessing_steps": {
        "global": [],
        "values": [],
        "target": []
      },
      "split": {
        "name": "random",
        "train_size": 0.7,
        "val_size": 0.15
      }
    },
    "type": {
      "name": "full_values_unbiased",
      "shuffle": true,
      "batch_size": 512,
      "lag_size": 512,
      "prediction_size": 1,
      "data_loader": {
        "num_workers": 6,
        "drop_last": false
      },
      "output_visualization": {
        "train_samples": 3,
        "val_samples": 6,
        "test_samples": 6,
        "rate_predictions": 2,
        "initial_color": "#c0e0de",
        "values_color": "gray",
        "target_color": "#3c474b",
        "prediction_color_1": "#4f7cac",
        "prediction_color_2": "#371e30"
      }
    }
  },
  "model": {
    "type": {
      "name": "neural_network",
      "train_info": {
        "number_epochs": 50,
        "early_stopping": true,
        "optimizer": {
          "name": "Adam",
          "args": {
            "lr": 0.01,
            "amsgrad": true
          }
        },
        "loss_function": {
          "name": "Default",
          "criterion": "mse"
        }
      },
      "transforms": [
        {
          "name": "weight_init",
          "type": "gaussian",
          "args": {
            "mean": 0,
            "std": 1
          }
        }
      ]
    },
    "network": {
      "name": "scinet",
      "args": {
        "hid_size": 1,
        "dropout": 0,
        "num_levels": 1
      }
    }
  },
  "logging": "INFO",
  "save_model": false,
  "save_charts": false,
  "compute_evaluation": false,
  "manual_seed": 0,
  "device": "cuda"
}