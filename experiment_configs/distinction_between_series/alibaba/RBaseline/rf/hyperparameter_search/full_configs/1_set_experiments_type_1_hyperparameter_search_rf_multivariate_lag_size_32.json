{
  "data": {
    "source": {
      "name": "alibaba_2018",
      "directory_path": "../input/data/third_preprocess_compressed_standarized",
      "time_feature": "time_stamp",
      "target_feature": "maximum_cpu_average_usage_relative",
      "batch_size": 512,
      "preprocessing_steps": {
        "global": [],
        "values": [],
        "target": []
      },
      "split": {
        "name": "random",
        "train_size": 0.7,
        "val_size": 0.15
      }
    },
    "type": {
      "name": "full_values_unbiased",
      "shuffle": true,
      "batch_size": 2048,
      "lag_size": 32,
      "prediction_size": 1,
      "data_loader": {
        "num_workers": 1,
        "drop_last": false
      },
      "output_visualization": {
        "train_samples": 3,
        "val_samples": 6,
        "test_samples": 6,
        "rate_predictions": 2,
        "initial_color": "#c0e0de",
        "values_color": "gray",
        "target_color": "#3c474b",
        "prediction_color_1": "#4f7cac",
        "prediction_color_2": "#371e30"
      }
    }
  },
  "model": {
    "type": {
      "name": "machine_learning_algorithm"
    },
    "algorithm": {
      "name": "random_forest",
      "args": {
        "with_search": {
          "n_iter": 20,
          "n_jobs": 1,
          "pre_dispatch": 2,
          "grid_params": {
            "n_estimators": [
              8,
              100,
              "uniform"
            ],
            "min_samples_leaf": [
              1,
              512,
              "uniform"
            ],
            "max_depth": [
              4,
              4096,
              "uniform"
            ],
            "max_features": [
              2,
              8,
              "uniform"
            ]
          },
          "default_params": {
            "criterion": "mse",
            "random_state": 0
          }
        }
      }
    }
  },
  "logging": "INFO",
  "save_model": true,
  "manual_seed": 0,
  "device": "cpu"
}